{
  "name": "afrihost/swarm-process",
  "description": "Much like the Process package, but this will run a list of processes, with a limited concurrent - and keep track of them",
  "minimum-stability": "dev",
  "license": "MIT",
  "keywords": ["process", "swarm", "bulk"],
  "authors": [
    {
      "name": "Sarel van der Walt",
      "email": "open-source@afrihost.com"
    }
  ],
  "require": {
    "symfony/process": "2.8.*|~3.0",
    "monolog/monolog": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6|^5",
    "symfony/console": "~3.4",
    "symfony/var-dumper": "~3.4"
  },
  "autoload": {
    "psr-4": {
      "Afrihost\\SwarmProcess\\": "src"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/alias.php"
    ]
  }
}
